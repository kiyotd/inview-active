{
  "name": "@kiyotd/scroll-toggle-active-class",
  "version": "1.0.21",
  "main": "dist/esm.min.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "kiyotd",
    "email": "torigoedesign+kiyotd@gmail.com",
    "url": "https://twitter.com/_kiyotd"
  },
  "description": "Utility",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiyotd/scroll-toggle-active-class.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts && tsc --noEmit",
    "prettier": "prettier --write src/**/*.ts",
    "dev": "run-p dev:*",
    "dev:browser": "npx esbuild src/index.ts --target=esnext --format=iife --bundle --outfile=dist/main.js --watch",
    "dev:esm": "npx esbuild src/index.ts --target=esnext --format=esm --bundle --outfile=dist/index.js --watch",
    "dev:sync": "browser-sync start --server --baseDir './' --startPath '/demo/local.html' --files 'demo/*' 'dist/**/*' --port 3010",
    "build": "npm-run-all build:*",
    "build:clean": "rimraf dist",
    "build:browser": "node esbuild/browser.mjs",
    "build:esm": "node esbuild/esm.mjs",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir './dist'",
    "publish": "npm publish --access public"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.1",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@typescript-eslint/typescript-estree": "^5.59.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.29.1",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2"
  },
  "volta": {
    "node": "18.16.0"
  }
}
